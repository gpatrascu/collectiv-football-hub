{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/*"
    ]
  },
  "auth": {
    "rolesSource": "/api/roles",
    "identityProviders": {
      "facebook": {
        "registration": {
          "clientIdSettingName": "FACEBOOK_APP_ID",
          "clientSecretSettingName": "FACEBOOK_APP_SECRET_APP_SETTING_NAME"
        }
      },
      "google": {
        "registration": {
          "clientIdSettingName": "GOOGLE_CLIENT_ID",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET_APP_SETTING_NAME"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/.auth/login/google/callback",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/.auth/login/facebook/callback",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/roles",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/google"
    }
  }
}
