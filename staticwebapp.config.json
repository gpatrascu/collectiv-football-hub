{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/.auth/*",
      "/favicon.ico"
    ]
  },
  "auth": {
    "rolesSource": "/api/getRolesForUser",
    "identityProviders": {
      "facebook": {
        "registration": {
          "clientIdSettingName": "FACEBOOK_APP_ID",
          "clientSecretSettingName": "FACEBOOK_APP_SECRET_APP_SETTING_NAME"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/.auth/*"
    }
  ]
}